{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[replace with domain]",
    "TenantId": "[replace with tenant id]",
    "ClientId": "[replace with client id]",
    "CallbackPath": "/signin-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CleanArchitectureConnectionsString": "Server=(localdb)\\mssqllocaldb;Database=CleanArchitecture;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Blobs": {
    "ServiceUri": "https://[replace with service URI].blob.core.windows.net/",
    "SourceBlobsContainer": "sa-mgdid-test",
    "PublishedBlobsContainer": "sa-mgdid-published-test"
  },
  "Email": {
    "Endpoint": "https://[replace with email domain].europe.communication.azure.com/",
    "Sender": "DoNotReply@[replace with sender email].azurecomm.net"
  },
  "ApplicationInsights": {
    "ConnectionString": "[replace with application insights connection string]"
  }
}